{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "target": "es5",

    "baseUrl": "src",

    "paths": {
      "src/*": ["src/*"],
      "components/*": ["src/components/*"],
      "style/*": ["style/*"],
      "images/*": ["images/*"],
      "pages/*": ["pages/*"],
      "statics/*": ["statics/*"],
      "apps/*": ["apps/*"],
      "common/*": ["common/*"]
    },
  }
}